<!-- flightDetails.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Flight Details</title>
</head>
<body>
    <h1>Flight Details</h1>
    <p>Client Name: <%= clientFlightDetails.name %></p>
    <p>Email: <%= clientFlightDetails.email %></p>
    <p>Flight Number: <%= clientFlightDetails.flightNumber %></p>
    <p>Origin: <%= arrivalDetails.origin %></p>
    <p>Scheduled Arrival Time: <%= arrivalDetails.scheduledArrivalTime %></p>
    <p>Estimated Arrival Time: <%= arrivalDetails.estimatedArrivalTime %></p>
    <p>Status: <%= arrivalDetails.status %></p>
    <p>Gate: <%= arrivalDetails.gate %></p>
    <p>Terminal: <%= arrivalDetails.terminal %></p>
</body>
</html>
